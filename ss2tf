
pkg load signal

% 1 - A
A = [0 1 0; 0 0 1; -1 -2 -3];
B = [10; 0; 0];
C = [1 0 0];
D = 0;


[numA, denA] = ss2tf(A, B, C, D);
disp("Numerador A:");
disp(numA);
disp("Denominador A :\n");
disp(denA);
disp("Denominador A :\n");
% 1 - A

% 1 - B
A = [-4 1.5; 4 0];
B = [2; 0];
C = [1.5 0.625];
D = 0;

[num, den] = ss2tf(A, B, C, D);
disp("Numerador B:");
disp(num);
disp("Denominador  B:");
disp(den);
% 1 - B
